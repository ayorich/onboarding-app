    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^8.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "prettier": "^3.4.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
